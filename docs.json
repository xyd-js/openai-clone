{
    "theme": {
        "name": "poetry",
        "logo": {
            "light": "/public/assets/logo.svg",
            "dark": "/public/assets/logo-dark.svg"
        },
        "favicon": "/public/assets/favicon.svg",
        "maxTocDepth": 2
    },
    "integrations": {
        "search": {
            "orama": true
        }
    },
    "navigation": {
        "anchors": {
            "header": [
                {
                    "title": "API Reference",
                    "page": "api-reference"
                }
            ]
        },
        "sidebar": [
            {
                "route": "api-reference",
                "pages": [
                    {
                        "group": "API Reference",
                        "sort": 0,
                        "pages": [
                            "api-reference/introduction"
                        ]
                    }
                ]
            }
        ]
    },
    "api": {
        "openapi": {
            "source": "./openapi.yaml",
            "route": "api-reference"
        }
    },
    "plugins": [
        "./plugins/pluginOasOpenai"
    ]
}